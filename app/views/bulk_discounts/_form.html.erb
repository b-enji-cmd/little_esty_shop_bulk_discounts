<%= form_with model: model, url: path, method: method, local: true do |f| %>
  <%= f.label "Quantity Threshold" %>
  <%= f.number_field "quantity_threshold",value: quan_value , within: 1...100 ,required: true %>
  <%= f.label "Percentage Discount" %>
  <%= f.number_field "percentage_discount",value: per_value, within: 1...100 ,required: true %>

  <%= f.submit button_text %>
<% end %>
